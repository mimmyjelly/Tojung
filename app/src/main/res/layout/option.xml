<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/optionmenu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="1dp"></ImageView>

    <ImageView
        android:layout_width="66dp"
        android:layout_height="64dp"
        android:src="@drawable/option"
        tools:layout_editor_absoluteX="276dp"
        tools:layout_editor_absoluteY="0dp"
        tools:ignore="MissingConstraints"></ImageView>


    <ImageView
        android:layout_width="66dp"
        android:layout_height="64dp"
        android:src="@drawable/protector"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="354dp"></ImageView>

    <ImageView
        android:layout_width="66dp"
        android:layout_height="64dp"
        android:src="@drawable/sun"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="93dp"></ImageView>

    <ImageView
        android:layout_width="66dp"
        android:layout_height="64dp"
        android:src="@drawable/sleeping"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="225dp"></ImageView>

</androidx.constraintlayout.widget.ConstraintLayout>